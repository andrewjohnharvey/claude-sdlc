{
  "mcpServers": {
    "Context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ],
      "env": {}
    },
    "Playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest"
      ],
      "env": {}
    },
    "Shadcn UI": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "shadcn-ui-mcp-server"
      ],
      "env": {}
    }
  }
}